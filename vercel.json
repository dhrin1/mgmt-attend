{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*.(js|css|png|jpe?g|gif|svg|woff2?|ttf|eot|json))$",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
